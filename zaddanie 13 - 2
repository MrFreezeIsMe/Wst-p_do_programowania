#include <iostream>
#include <vector>

using namespace std;

int onlyPositive(vector<int> wektor)
{
	int counter = 0;
	for (int i = 0; i < wektor.size(); i++)
	{
		if (wektor.at(i) > 0)
		{
			counter++;
		}
	}
	return counter;
}

int main()
{
	srand(time(NULL));
	vector<int> wektor = { -3, 2, 3, -15, 22, 0 };

	cout << onlyPositive(wektor);
}
