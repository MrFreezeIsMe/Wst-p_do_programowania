#include <iostream>

using namespace std;
int i;
int j = 1;
int k = 1;

int main(){
    cout << "Podaj liczbe, ktorej silnie chcesz wyliczyc: \n";
        cin >> i;

        if ((i == 0) || (i == 1))
        {
            cout << 1;
        }
        else
        {
            while (k <= i)
            {
                j = j * k;
                k++;
            }
            cout << j;
        }
    
}
