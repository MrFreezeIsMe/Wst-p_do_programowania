#include <iostream>

using namespace std;

int parzyste[5] = { 2,4,6,8,10 };


int main()
{
    cout << "Length of an array: " << sizeof(parzyste)/4;
}


#include <iostream>

using namespace std;

int parzyste[5] = { 2,4,6,8,10 };


int main()
{
    cout << "Length of an array: " << sizeof(parzyste) / sizeof(int);
}

