#include <iostream>

using namespace std;

void zamien(int a, int b)
{
    int temp = a;
    a = b;
    b = temp;

}

void zamienPtr(int *a, int *b)
{
    int *temp = a;
    a = b;
    b = temp;
}

int main()
{
    int a = 4;
    int b = 3;
    zamien(a, b);
    cout << a << " " << b << endl;
    zamienPtr(&a, &b)
    cout << a << " " << b;
}
